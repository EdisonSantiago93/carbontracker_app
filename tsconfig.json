{
  "compilerOptions": {
    "target": "esnext",
  "module": "NodeNext",
  "lib": ["es6"],
    "allowJs": true,
  "jsx": "react-jsx",
  "jsxImportSource": "react",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "nodenext",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "include": [
    "src/**/*"
  ],
  "extends": "expo/tsconfig.base"
}